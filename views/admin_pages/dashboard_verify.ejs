<h2 class="font-bold mt-4 pb-2 border-b border-gray-200">All verification requests made to this organization:</h2>
<div class="mt-8 grid grid-cols-3">
    
    <% for (var i=0; i < resp_data.length; i++) {%>

    <div class="card rounded-3xl shadow-md border-t-4 mx-8 my-4">
        <div class="m-8">
          <!-- TODO: Load subjects from a org-to-subject list -->
          <span class="font-bold text-lg">Username: <%= resp_data[i].username %></span>
    
          <div class="w-100 flex">
           
              <span class="block text-gray-500 text">Email: <%= resp_data[i].email %></span>
              <span class="block text-gray-500 text"></span>
          
            
          </div>
        <div class="mx-auto w-max"><a href="verify?user=<%= resp_data[i].username %>"><div class="bg-blue-300 mt-8 rounded-full py-2 px-12 font-bold text-white text hover:bg-blue-500 transition- duration-500">Verify</div></a></div>
         
        </div>
      </div>
      
    <% } %>
      
</div>